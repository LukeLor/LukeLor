local Spawner = loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Utilities/main/Doors%20Entity%20Spawner/Source.lua"))()


-- Create entity
local entityTable = Spawner.createEntity({
    CustomName = "MadGuy", -- Custom name of your entity
    Model = "rbxassetid://15850594174", -- Can be GitHub file or rbxassetid
    Speed = 12.5, -- Percentage, 100 = default Rush speed
    DelayTime = 0, -- Time before starting cycles (seconds)
    HeightOffset = 0,
    CanKill = false,
    KillRange = 50,
    BackwardsMovement = false,
    BreakLights = false,
    FlickerLights = {
        true, -- Enabled/Disabled
        4, -- Time (seconds)
    },
    Cycles = {
        Min = 1,
        Max = 1,
        WaitTime = 0,
    },
    CamShake = {
        true, -- Enabled/Disabled
        {3.5, 20, 0.1, 1}, -- Shake values (don't change if you don't know)
        50, -- Shake start distance (from Entity to you)
    },
    Jumpscare = {
        true, -- Enabled/Disabled
        {
            Image1 = "rbxassetid://12875044447", -- Image1 url
            Image2 = "rbxassetid://12875044447", -- Image2 url
            Shake = true,
            Sound1 = {
                12875044447, -- SoundId
                { Volume = 0.5 }, -- Sound properties
            },
            Sound2 = {
                5546573724, -- SoundId
                { Volume = 0.5 },
                { PlaybackSpeed = 0.3 },-- Sound properties
            },
            Flashing = {
                true, -- Enabled/Disabled
                Color3.fromRGB(220, 110, 110), -- Color
            },
            Tease = {
                false, -- Enabled/Disabled
                Min = 1,
                Max = 6,
            },
        },
    },
    CustomDialog = {"You died to Rush.", "This is quite an unexpected death.", "It's an alternative version.", "He is way much faster!"}, -- Custom death message
})


-----[[  Debug -=- Advanced  ]]-----
entityTable.Debug.OnEntitySpawned = function()
    print("Entity has spawned:", entityTable)
end

entityTable.Debug.OnEntityDespawned = function()
    print("Entity has despawned:", entityTable)
local SD = Instance.new("Sound")
SD.SoundId = 3908308607
SD.Parent = Model
SD:Play()
SD.PlaybackSpeed = 0.3
end
entityTable.Debug.OnEntityStartMoving = function()
    print("Entity has started moving:", entityTable)
end

entityTable.Debug.OnEntityFinishedRebound = function()
    print("Entity has finished rebound:", entityTable)
end

entityTable.Debug.OnEntityEnteredRoom = function(room)
    print("Entity:", entityTable, "has entered room:", room)
end

entityTable.Debug.OnLookAtEntity = function()
    print("Player has looked at entity:", entityTable)
      local plrname = game.Players.LocalPlayer.Name
    local plrmodel = workspace:FindFirstChild(plrname)
    plrmodel.Humanoid.Health -= 5
end

entityTable.Debug.OnDeath = function()
    warn("Player has died.")
end
------------------------------------
-- Run the created entity
Spawner.runEntity(entityTable)
