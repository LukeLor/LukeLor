--[[

    NOTE: Custom shop items will NOT cost any real knobs

]]--

local Functions = loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Utilities/main/Functions.lua"))()
local CustomShop = loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Utilities/main/Doors/Custom%20Shop%20Items/Source.lua"))()

-- Create your tool here
local exampleTool = LoadCustomInstance("rbxassetid://15863226220")
--exampleTool.TextureId = "rbxassetid://15875523650"


-- Create custom shop item
CustomShop.CreateItem(exampleTool, {
    Title = "Ultimate Crucifix",
    Desc = "Crucify those fanmade Entities back where they can from",
    Image = "https://tr.rbxcdn.com/b4b52ba92404af735a6c9f3502c2d78b/150/150/Decal/Png",
    Price = 0,
    Stack = 100000,
})

