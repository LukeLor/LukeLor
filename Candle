
local Uc = game:GetObject("rbxassetid://17363284096")[1]
Uc.Parent = game.Players.LocalPlayer.Backpack

while true do 
	wait(0.1)
	if workspace:FindFirstChild("A-12") then
Uc.Handle.Attachment.FireParticles.Enabled = false
Uc.Handle.Attachment.Smoke.Enabled = false
Uc.Handle.Attachment.SparkParticles.Enabled = false
Uc.Handle.Attachment.MainLight.Enabled = false
Uc.Handle.Attachment.Out1.Enabled = true
		wait(2)
Uc.Handle.Attachment.Out1.Enabled = false
	end
	if not workspace:FindFirstChild("A-12") then
Uc.Handle.Attachment.FireParticles.Enabled = true
Uc.Handle.Attachment.Smoke.Enabled = true
Uc.Handle.Attachment.SparkParticles.Enabled = true
Uc.Handle.Attachment.MainLight.Enabled = true
	end
end
